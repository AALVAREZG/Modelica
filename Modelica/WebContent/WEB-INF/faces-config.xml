<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">
    <application>
 <action-listener>
    org.primefaces.application.DialogActionListener
  </action-listener>
  <navigation-handler>
    org.primefaces.application.DialogNavigationHandler
  </navigation-handler>
  <view-handler>
    org.primefaces.application.DialogViewHandler
  </view-handler>
</application> 
<navigation-rule>
        <from-view-id>/*</from-view-id>
        <navigation-case>
         	<from-action>#{asistenteLicenciaMB.nuevo}</from-action>
            <from-outcome>OBRAMAYOR</from-outcome>
            <to-view-id>/front/f-obramayor.xhtml</to-view-id>
       </navigation-case>
       <navigation-case>
         	<from-action>#{asistenteLicenciaMB.nuevo}</from-action>
            <from-outcome>OBRAmenor</from-outcome>
            <to-view-id>/front/f-obramenor.xhtml</to-view-id>
       </navigation-case>
        <navigation-case>
            <from-action>#{asistenteLicenciaMB.nuevo}</from-action>
            <from-outcome>OCUPACION</from-outcome>
            <to-view-id>/front/f-ocupacion.xhtml</to-view-id>
       </navigation-case>
        <navigation-case>
            <from-action>#{asistenteLicenciaMB.nuevo}</from-action>
            <from-outcome>SEGREGACION</from-outcome>
            <to-view-id>/front/f-segregacion.xhtml</to-view-id>
       </navigation-case>
       <navigation-case>
            <from-action>#{asistenteLicenciaMB.nuevo}</from-action>
            <from-outcome>PLANEAMIENTO</from-outcome>
            <to-view-id>/front/f-planeamiento.xhtml</to-view-id>
       </navigation-case>
       <navigation-case>
            <from-action>#{asistenteLicenciaMB.nuevo}</from-action>
            <from-outcome>CERTIFICADO</from-outcome>
            <to-view-id>/front/f-planeamiento.xhtml</to-view-id>
       </navigation-case>
        <navigation-case>
            <from-action>#{asistenteLicenciaMB.nuevo}</from-action>
            <from-outcome>OTROS</from-outcome>
            <to-view-id>/front/f-planeamiento.xhtml</to-view-id>
       </navigation-case>
</navigation-rule>

<application>
        <resource-bundle>
            <base-name>/Bundle</base-name>
            <var>bundle</var>
        </resource-bundle>
        <resource-bundle>
            <base-name>/Bundle</base-name>
            <var>bundle</var>
        </resource-bundle>
    </application>
</faces-config>
